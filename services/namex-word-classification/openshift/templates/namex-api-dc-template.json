{
    "kind": "Template",
    "apiVersion": "v1",
    "metadata": {
        "name": "namex-api-dc-template",
        "creationTimestamp": null
    },
    "objects": [
        {
            "kind": "DeploymentConfig",
            "apiVersion": "v1",
            "metadata": {
                "name": "namex-api",
                "generation": 1,
                "creationTimestamp": null,
                "labels": {
                    "app": "namex-api"
                },
                "annotations": {
                    "openshift.io/generated-by": "OpenShiftWebConsole"
                }
            },
            "spec": {
                "strategy": {
                    "type": "Rolling",
                    "rollingParams": {
                        "updatePeriodSeconds": 1,
                        "intervalSeconds": 1,
                        "timeoutSeconds": 600,
                        "maxUnavailable": "25%",
                        "maxSurge": "25%",
                        "pre": {
                            "failurePolicy": "Abort",
                            "execNewPod": {
                                "command": [
                                    "/opt/app-root/src/setup.sh"
                                ],
                                "containerName": "namex-api"
                            }
                        }
                    },
                    "resources": {},
                    "activeDeadlineSeconds": 21600
                },
                "triggers": [
                    {
                        "type": "ImageChange",
                        "imageChangeParams": {
                            "automatic": true,
                            "containerNames": [
                                "namex-api"
                            ],
                            "from": {
                                "kind": "ImageStreamTag",
                                "namespace": "servicebc-ne-tools",
                                "name": "namex-api:latest"
                            }
                        }
                    },
                    {
                        "type": "ConfigChange"
                    }
                ],
                "replicas": 1,
                "test": false,
                "selector": {
                    "app": "namex-api",
                    "deploymentconfig": "namex-api"
                },
                "template": {
                    "metadata": {
                        "creationTimestamp": null,
                        "labels": {
                            "app": "namex-api",
                            "deploymentconfig": "namex-api"
                        },
                        "annotations": {
                            "openshift.io/generated-by": "OpenShiftWebConsole"
                        }
                    },
                    "spec": {
                        "volumes": [
                            {
                                "name": "volume-92eu8",
                                "configMap": {
                                    "name": "namex-api-keycloak",
                                    "defaultMode": 420
                                }
                            }
                        ],
                        "containers": [
                            {
                                "name": "namex-api",
                                "image": "172.50.0.2:5000/servicebc-ne-tools/namex-api@sha256:ada70eec46d95487dea5dbf870a937ed91c8dd38c3303d264cf8980dae5aee6c",
                                "ports": [
                                    {
                                        "containerPort": 8080,
                                        "protocol": "TCP"
                                    }
                                ],
                                "envFrom": [
                                    {
                                        "configMapRef": {
                                            "name": "namex-api-config"
                                        }
                                    }
                                ],
                                "env": [
                                    {
                                        "name": "DATABASE_NAME",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql",
                                                "key": "database-name"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_USERNAME",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql",
                                                "key": "database-user"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_PASSWORD",
                                        "valueFrom": {
                                            "secretKeyRef": {
                                                "name": "postgresql",
                                                "key": "database-password"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_HOST",
                                        "valueFrom": {
                                            "configMapKeyRef": {
                                                "name": "namex-api-config",
                                                "key": "DB_HOST"
                                            }
                                        }
                                    },
                                    {
                                        "name": "DATABASE_PORT",
                                        "valueFrom": {
                                            "configMapKeyRef": {
                                                "name": "namex-api-config",
                                                "key": "DB_PORT"
                                            }
                                        }
                                    }
                                ],
                                "resources": {},
                                "volumeMounts": [
                                    {
                                        "name": "volume-92eu8",
                                        "mountPath": "/opt/app-root/src/client_secrets"
                                    }
                                ],
                                "livenessProbe": {
                                    "httpGet": {
                                        "path": "/api/v1/healthz",
                                        "port": 8080,
                                        "scheme": "HTTP"
                                    },
                                    "timeoutSeconds": 2,
                                    "periodSeconds": 10,
                                    "successThreshold": 1,
                                    "failureThreshold": 3
                                },
                                "readinessProbe": {
                                    "httpGet": {
                                        "path": "/api/v1/readyz",
                                        "port": 8080,
                                        "scheme": "HTTP"
                                    },
                                    "timeoutSeconds": 2,
                                    "periodSeconds": 10,
                                    "successThreshold": 1,
                                    "failureThreshold": 3
                                },
                                "terminationMessagePath": "/dev/termination-log",
                                "terminationMessagePolicy": "File",
                                "imagePullPolicy": "Always"
                            }
                        ],
                        "restartPolicy": "Always",
                        "terminationGracePeriodSeconds": 30,
                        "dnsPolicy": "ClusterFirst",
                        "securityContext": {},
                        "schedulerName": "default-scheduler"
                    }
                }
            },
            "status": {
                "latestVersion": 0,
                "observedGeneration": 0,
                "replicas": 0,
                "updatedReplicas": 0,
                "availableReplicas": 0,
                "unavailableReplicas": 0
            }
        }
    ]
}
